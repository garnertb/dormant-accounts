{
  "name": "@dormant-accounts/github",
  "version": "0.2.1",
  "description": "A GitHub integration for dormant accounts management.",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./copilot": {
      "import": "./dist/copilot.js",
      "require": "./dist/copilot.cjs",
      "types:": "./dist/copilot.*.ts"
    }
  },
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts",
        "dist/index.d.cts"
      ],
      "copilot": [
        "dist/copilot.d.ts",
        "dist/copilot.d.cts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rimraf dist && tsup",
    "dev": "tsup --watch --clean=false",
    "eslint": "eslint-runner",
    "eslint:fix": "eslint-runner --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@actions/github": "^6.0.1",
    "@octokit/plugin-throttling": "11.0.1",
    "@types/ms": "2.1.0",
    "ms": "2.1.3"
  },
  "devDependencies": {
    "@octokit/types": "14.1.0",
    "@types/node": "24.7.0",
    "dormant-accounts": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "rimraf": "6.0.1",
    "tsconfig": "workspace:*",
    "tsup": "8.5.0",
    "type-fest": "4.41.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
