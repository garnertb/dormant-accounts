{
  "name": "@dormant-accounts/github",
  "version": "0.2.1",
  "description": "A GitHub integration for dormant accounts management.",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./copilot": {
      "types": "./dist/copilot.d.mts",
      "import": "./dist/copilot.mjs",
      "require": "./dist/copilot.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts",
        "dist/index.d.cts"
      ],
      "copilot": [
        "dist/copilot.d.ts",
        "dist/copilot.d.cts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rimraf dist && tsdown",
    "dev": "tsdown --watch --clean=false",
    "eslint": "eslint-runner",
    "eslint:fix": "eslint-runner --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@actions/github": "^6.0.1",
    "@types/ms": "2.1.0",
    "dormant-accounts": "workspace:*",
    "ms": "2.1.3"
  },
  "devDependencies": {
    "@octokit/types": "16.0.0",
    "@types/node": "24.9.2",
    "eslint-config-custom": "workspace:*",
    "rimraf": "6.1.0",
    "tsconfig": "workspace:*",
    "tsdown": "^0.18.0",
    "type-fest": "5.2.0",
    "typescript": "5.9.3",
    "vitest": "4.0.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
