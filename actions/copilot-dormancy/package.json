{
  "name": "@dormant-accounts/copilpot-dormancy-action",
  "version": "0.1.10",
  "description": "A GitHub Action to check for dormant accounts in a GitHub organization.",
  "keywords": [
    "github",
    "action",
    "dormant",
    "accounts"
  ],
  "license": "MIT",
  "author": "Tyler Garner",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      ".": [
        "dist/*.d.ts",
        "dist/*.d.cts"
      ]
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "rimraf dist && ncc build src/index.ts -o dist",
    "dev": "npx nodemon --watch src --ext ts --exec 'pnpm build'",
    "eslint": "eslint-runner",
    "eslint:fix": "eslint-runner --fix",
    "test": "vitest --run",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@actions/core": "1.11.1",
    "@actions/github": "^6.0.1",
    "@octokit/plugin-throttling": "11.0.2",
    "@types/ms": "2.1.0",
    "ms": "2.1.3",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@dormant-accounts/github": "workspace:*",
    "@types/node": "24.7.0",
    "@vercel/ncc": "0.38.4",
    "dormant-accounts": "workspace:*",
    "eslint-config-custom": "workspace:*",
    "rimraf": "6.0.1",
    "tsconfig": "workspace:*",
    "tsup": "8.5.0",
    "type-fest": "4.41.0",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
